    "test": "jest",
    "lint": "standard 'src/**/*.js' 'src/*.js' '*.js'",
    "release": "standard-version -i changelog.md -m '%s'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/relekang/micro-rss-parser.git"
  },
  "author": "Rolf Erik Lekang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/relekang/micro-rss-parser/issues"
  },
  "homepage": "https://github.com/relekang/micro-rss-parser#readme",
  "jest": {
    "coverageDirectory": "./coverage/",
    "testRegex": "/__tests__/.*\\.tests\\.js$"
  },
  "dependencies": {
    "args": "^3.0.4",
    "cheerio": "^1.0.0-rc.2",
    "express-graphql": "^0.6.7",
    "feedme": "^1.0.0",
    "graphql": "^0.11.0",
    "is-url": "^1.2.2",
    "lodash-fp": "^0.10.4",
    "micro": "^8.0.0",
    "node-feedparser": "^1.0.1",
    "normalize-url": "^1.9.1",
    "raven": "^2.1.1",
    "rss-parser": "^2.10.1",
    "superagent": "^3.5.2",
    "update-notifier": "^2.1.0"
  },
  "devDependencies": {
    "fs-extra-promise": "^1.0.1",
    "jest": "^20.0.4",
    "nock": "^9.0.14",
    "nodemon": "^1.11.0",
    "prettier": "^1.5.3",
    "standard": "^10.0.2",
