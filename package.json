    "start": "./cli.js",
    "dev": "nodemon ./dev.js",
    "test": "jest",
    "lint": "eslint --ext ts,js src *.js",
    "format": "yarn lint -- --fix",
    "release": "yarn build && standard-version -i changelog.md -m '%s'"
